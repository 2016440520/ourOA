<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.springjdbc.mapper.UserLogMapper">
    <insert id="insert">
        insert into userlog(action,userName,ip,time)
        values(#{action},#{userName},#{ip},#{time})
    </insert>
</mapper>